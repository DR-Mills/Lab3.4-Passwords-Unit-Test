	public static boolean isLength7to12(String str) {
		return (str.length() >= 7 && str.length() <= 12);
	}
	
	public static boolean hasNumberExcluding6(String str) {
		Pattern numsNot6 = new Pattern.compile("[0-5[7-9]]");
		Matcher match = numsNot6
	}
}
